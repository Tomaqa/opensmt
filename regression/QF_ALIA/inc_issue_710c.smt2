(set-logic QF_ALIA)
(declare-fun n () Int)
(declare-fun m () Int)
(declare-fun x () Int)
(declare-fun a () (Array Int Int))
(push 1)
(assert (= 0 (select a m)))
(assert (= a (store a 0 (select (store a n 0) m))))
(check-sat)
(pop 1)
(assert (not (= n m)))
(assert (= m (select a x)))
(assert (= n (select a x)))
(check-sat)
